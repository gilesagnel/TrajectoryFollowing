<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Rviz Color Properties -->
  <xacro:include filename="$(find robot)/urdf/includes/materials/materials.xacro" />  
  <!-- Rviz Color Properties -->  

  <!-- Gazebo Properties -->
  <xacro:include filename="$(find robot)/urdf/includes/gazebo/jetbot.gazebo.xacro" />  
  <!-- Gazebo Properties -->  

  <!-- Transmission Macro -->
  <xacro:include filename="$(find robot)/urdf/includes/gazebo/transmission.xacro" />    
  <!-- Transmission Macro -->

  <!-- Inertia Macros -->
  <xacro:include filename="$(find robot)/urdf/includes/gazebo/inertias.xacro" />  
  <!-- Inertia Macros -->

  <!-- Chassis macro -->
  <xacro:include filename="$(find robot)/urdf/includes/parts/chassis.xacro" />  
  <!-- Chassis macro -->

  <!-- Wheel Macro -->
  <xacro:include filename="$(find robot)/urdf/includes/parts/wheels/wheel.xacro" />  
  <!-- Wheel Macro -->

  <!-- Caster Wheel Macro -->
  <xacro:include filename="$(find robot)/urdf/includes/parts/wheels/caster.xacro" />  
  <!-- Caster Wheel Macro -->

  <!-- Camera-->
  <xacro:include filename="$(find robot)/urdf/includes/parts/sensors/rgbcam/camera.xacro" />
  <!-- Camera-->

  <!-- PCB Boards -->
  <xacro:include filename="$(find robot)/urdf/includes/parts/boards.xacro" />  
  <!-- PCB Boards -->


</robot>